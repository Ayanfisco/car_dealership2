<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ir_cron_dealership_vehicle_reminder" model="ir.cron">
        <field name="name">Dealership Vehicle Maintenance Reminder</field>
        <field name="model_id" ref="model_dealership_vehicle"/>
        <field name="state">code</field>
        <field name="code">model._send_maintenance_reminders()</field>
        <field name="interval_number">7</field>
        <field name="interval_type">days</field>
        <field name="active" eval="True"/>
    </record>
</odoo>
