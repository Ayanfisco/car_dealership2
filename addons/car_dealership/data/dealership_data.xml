<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Stock Input Account -->
    <record id="account_dealership_stock_input" model="account.account">
        <field name="name">Dealership Stock Input</field>
        <field name="code">50100</field>
        <field name="account_type">asset_current</field>
        <field name="reconcile">False</field>
    </record>

    <!-- Stock Output Account -->
    <record id="account_dealership_stock_output" model="account.account">
        <field name="name">Dealership Stock Output</field>
        <field name="code">50200</field>
        <field name="account_type">asset_current</field>
        <field name="reconcile">False</field>
    </record>

    <!-- Stock Valuation Account -->
    <record id="account_dealership_stock_valuation" model="account.account">
        <field name="name">Dealership Stock Valuation</field>
        <field name="code">50300</field>
        <field name="account_type">asset_current</field>
        <field name="reconcile">False</field>
    </record>

    <!-- Commission Account -->
    <record id="account_dealership_commission" model="account.account">
        <field name="name">Dealership Commission</field>
        <field name="code">42100</field>
        <field name="account_type">expense</field>
        <field name="reconcile">False</field>
    </record>

    <!-- Income Account -->
    <record id="account_dealership_income" model="account.account">
        <field name="name">Dealership Income</field>
        <field name="code">41100</field>
        <field name="account_type">income</field>
        <field name="reconcile">False</field>
    </record>

    <!-- Other Income Account -->
    <record id="account_other_income" model="account.account">
        <field name="name">Other Income</field>
        <field name="code">41200</field>
        <field name="account_type">income</field>
        <field name="reconcile">False</field>
    </record>
</odoo>