<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="demo_dealer_partner" model="res.partner">
        <field name="name">Demo Dealer</field>
        <field name="is_company" eval="True"/>
    </record>
    <record id="demo_consignor_partner" model="res.partner">
        <field name="name">Demo Consignor</field>
        <field name="is_company" eval="True"/>
    </record>
    <record id="demo_vehicle_owner" model="dealership.vehicle">
        <field name="name">Demo Owner Car</field>
        <field name="vin_number">VINOWN123456</field>
        <field name="make_id" ref="fleet.model_brand_toyota"/>
        <field name="model_id" ref="fleet.model_model_corolla"/>
        <field name="year">2024</field>
        <field name="business_type">owner</field>
        <field name="purchase_price">10000</field>
        <field name="selling_price">12000</field>
    </record>
    <record id="demo_vehicle_dealer" model="dealership.vehicle">
        <field name="name">Demo Dealer Car</field>
        <field name="vin_number">VINDEAL123456</field>
        <field name="make_id" ref="fleet.model_brand_toyota"/>
        <field name="model_id" ref="fleet.model_model_corolla"/>
        <field name="year">2023</field>
        <field name="business_type">dealer_network</field>
        <field name="vendor_id" ref="demo_dealer_partner"/>
        <field name="commission_type">percentage</field>
        <field name="commission_value">5</field>
        <field name="purchase_price">9000</field>
        <field name="selling_price">11000</field>
    </record>
    <record id="demo_vehicle_consigned" model="dealership.vehicle">
        <field name="name">Demo Consigned Car</field>
        <field name="vin_number">VINCON123456</field>
        <field name="make_id" ref="fleet.model_brand_toyota"/>
        <field name="model_id" ref="fleet.model_model_corolla"/>
        <field name="year">2022</field>
        <field name="business_type">consigned</field>
        <field name="vendor_id" ref="demo_consignor_partner"/>
        <field name="commission_type">fixed</field>
        <field name="commission_value">500</field>
        <field name="purchase_price">8000</field>
        <field name="selling_price">9500</field>
    </record>
</odoo>
