<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Custom Vehicle States -->
        <record id="fleet_vehicle_state_certified" model="fleet.vehicle.state">
            <field name="name">Certified Pre-Owned</field>
            <field name="sequence">15</field>
        </record>

        <record id="fleet_vehicle_state_in_preparation" model="fleet.vehicle.state">
            <field name="name">In Preparation</field>
            <field name="sequence">16</field>
        </record>

        <record id="fleet_vehicle_state_reserved" model="fleet.vehicle.state">
            <field name="name">Reserved</field>
            <field name="sequence">17</field>
        </record>

        <record id="fleet_vehicle_state_test_drive" model="fleet.vehicle.state">
            <field name="name">In Test Drive</field>
            <field name="sequence">18</field>
        </record>

        <record id="fleet_vehicle_state_delivered" model="fleet.vehicle.state">
            <field name="name">Delivered</field>
            <field name="sequence">19</field>
        </record>
    </data>
</odoo>
