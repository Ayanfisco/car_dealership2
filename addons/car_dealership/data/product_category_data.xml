<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Main Dealership Category -->
    <record id="product_category_dealership_vehicles" model="product.category">
        <field name="name">Vehicles</field>
        <field name="property_cost_method">fifo</field>
        <field name="property_valuation">real_time</field>
        <field name="property_stock_account_input_categ_id" ref="account_dealership_stock_input"/>
        <field name="property_stock_account_output_categ_id" ref="account_dealership_stock_output"/>
        <field name="property_stock_valuation_account_id" ref="account_dealership_stock_valuation"/>
    </record>
</odoo>