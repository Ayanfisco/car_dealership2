<odoo>
    <record id="view_car_service_tree" model="ir.ui.view">
        <field name="name">car.service.tree</field>
        <field name="model">car.dealership.service</field>
        <field name="arch" type="xml">
            <tree>
                <field name="service_date"/>
                <field name="service_type"/>
                <field name="cost"/>
                <field name="description"/>
            </tree>
        </field>
    </record>
    <record id="view_car_service_form" model="ir.ui.view">
        <field name="name">car.service.form</field>
        <field name="model">car.dealership.service</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="car_id"/>
                        <field name="service_date"/>
                        <field name="service_type"/>
                        <field name="cost"/>
                        <field name="currency_id" invisible="1"/>
                        <field name="description"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="action_car_service" model="ir.actions.act_window">
        <field name="name">Service History</field>
        <field name="res_model">car.dealership.service</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>